extends ../works

block title
	title Isaac Paavola | Recent work

block body
	body.study.more.recent

block nav
	nav
		ul
			li#nava.nava
				include ../pieces/pvl
			li.navb.caps
				a.navlink(data-pathname="huddlepass")
					span &#8249;
					| Last Project
			li.navc.caps
				a.navlink(data-pathname="socialcrunch")
					| Next Project
					span &#8250;

block header
	header.more
		.wrap
			.logo
				span#logo(data-speed="5.55")
					img.fadeindownb(src="/images/recent/logo.png")
			.title
				span#title(data-speed="2.85")
					h1.fadeinup Nunc rutrum mi molestie faucibus erat at feugiat erat.
	.push
		div

block overview
	.arrowbar
		.arrowb
	.head
		span.fadeinleft
		h5.caps.fadeinup Overview
		span.fadeinright
	.wrap.hidden
		.overview
			.detail.single
				p A more casual place for me to throw up a few quick looks of newer things I've been on, even if unorganized or unfinished.

